{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "ctx7sk-f553b959-d33a-43c5-a185-3f44c406eeb4"
      }
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {}
    },
    "shrimp-task-manager": {
      "type": "stdio",
      "command": "node",
      "args": ["../../../mcp-shrimp-task-manager/dist/index.js"],
      "env": {
        "DATA_DIR": "./shrimp_data",
        "TEMPLATES_USE": "en",
        "ENABLE_GUI": "true"
      }
    }
  }
}
